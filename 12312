if isCommand("unhide") then
    game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("Unhided")
  game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
    local originalCollideState = {}

    for _, v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
        if v:IsA("BasePart") then
            originalCollideState[v] = v.CanCollide
            v.CanCollide = false
        end
    end

    noclipConnection = game:GetService("RunService").Stepped:Connect(function()
        for _, v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
            if v:IsA("BasePart") then
                v.CanCollide = false
            end
        end
    end)

    local tween = ts:Create(root, info, {CFrame = root.CFrame + Vector3.new(0, 16, 0)})
    tween:Play()
    tween.Completed:Connect(function()
        if noclipConnection then
            noclipConnection:Disconnect()
        end

        for _, v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
            if v:IsA("BasePart") and originalCollideState[v] ~= nil then
                v.CanCollide = originalCollideState[v]
            end
        end
        originalCollideState = {}
    end)
end

if isCommand("hide") then
    game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("Hided")
    local originalCollideState = {}

    for _, v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
        if v:IsA("BasePart") then
            originalCollideState[v] = v.CanCollide
            v.CanCollide = false
        end
    end

    noclipConnection = game:GetService("RunService").Stepped:Connect(function()
        for _, v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
            if v:IsA("BasePart") then
                v.CanCollide = false
            end
        end
    end)

    local tween = ts:Create(root, info, {CFrame = root.CFrame - Vector3.new(0, 6.7, 0)})
    tween:Play()
    tween.Completed:Connect(function()
    task.wait(0.040)
        game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
        if noclipConnection then
            noclipConnection:Disconnect()
        end

        for _, v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
            if v:IsA("BasePart") and originalCollideState[v] ~= nil then
                v.CanCollide = originalCollideState[v]
            end
        end
        originalCollideState = {}
    end)
end

local DanceAnims = {
    dolphin = "5918726674",
    monkey = "3333499508",
    floss = "5917459365",
    shuffle = "4349242221",
}

local function PlayDance(danceName)
    local player = game.Players.LocalPlayer
    local char = player.Character
    if not char or not char:FindFirstChild("Humanoid") then return end
    pcall(function()
        if CurrAnim and CurrAnim.IsPlaying then
            CurrAnim:Stop()
        end
        local anim = Instance.new("Animation")
        anim.AnimationId = "http://www.roblox.com/asset/?id=" .. DanceAnims[danceName]
        CurrAnim = char.Humanoid:FindFirstChildOfClass("Animator"):LoadAnimation(anim)
        CurrAnim:Play()
        CurrAnim:AdjustSpeed(1)
    end)
end

local function StopDance()
    local player = game.Players.LocalPlayer
    local char = player.Character
    if not char or not char:FindFirstChild("Humanoid") then return end
    pcall(function()
        if CurrAnim and CurrAnim.IsPlaying then
            CurrAnim:Stop()
        end
    end)
end

if isCommand("dolphin") then
    game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("Dancing dolphin")
    PlayDance("dolphin")
end

if isCommand("monkey") then
    game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("Dancing monkey")
    PlayDance("monkey")
end

if isCommand("floss") then
    game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("Dancing floss")
    PlayDance("floss")
end

if isCommand("shuffle") then
    game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("Dancing shuffle")
    PlayDance("shuffle")
end

if isCommand("stopdance") then
    game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("Dancing stopped")
    StopDance()
end

