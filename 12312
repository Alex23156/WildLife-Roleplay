    game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("FPS set")
    local fps = tonumber(SplitCMD[2]) or 3
    pcall(function()
        setfpscap(fps)
    end)
end

    if isCommand("wallet") then
        game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("Wallet on")
        game.Players.LocalPlayer.Backpack:FindFirstChild("[Wallet]").Parent = game.Players.LocalPlayer.Character
    end
    
    if isCommand("unwallet") then
        game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("Wallet off")
        game.Players.LocalPlayer.Character:FindFirstChild("[Wallet]").Parent = game.Players.LocalPlayer.Backpack
    end
    
    if isCommand("freeze") then
        game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("Freezed")
        game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
    end
    
    if isCommand("unfreeze") or isCommand("thaw") then
        game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("Unfreezed")
        game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
    end

local function convertToNumber(str)
    if not str then return nil end
    local num = tonumber(str:match("%d+"))
    local suffix = str:match("%a")
    if not num then return nil end
    if suffix == "k" then
        return num * 1000
    elseif suffix == "m" then
        return num * 1000000
    else
        return tonumber(str)
    end
end

local function getGroundMoney()
	local cash = 0
	for _, v in pairs(game:GetService("Workspace").Ignored.Drop:GetChildren()) do
		if v.Name == "MoneyDrop" then
			local subbed = string.gsub(v.BillboardGui.TextLabel.Text, "%D", "")
			cash = cash + tonumber(subbed) or 0
		end
	end
	return cash
end

if isCommand("drop") then
    game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("Started dropping")
	if not Dropping then
		Dropping = true
		local LossAmount = convertToNumber(SplitCMD[2])            
		while Dropping do
			local DroppedAmount = getGroundMoney()
			if LossAmount and DroppedAmount >= LossAmount then
				Dropping = false
                game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Successfully dropped " .. tostring(LossAmount), "All")
			end
			game.ReplicatedStorage.MainEvent:FireServer("DropMoney", tostring(15000))
			task.wait()
		end
	end
end

if isCommand("stop") then
    game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("Stopped dropping")
    Dropping = false
end

local Players = game:GetService("Players")
local TextChatService = game:GetService("TextChatService")

local function formatAbbrev(num)
    if num >= 1e9 then
        return string.format("%.1fB", num / 1e9):gsub("%.0B","B")
    elseif num >= 1e6 then
        return string.format("%.1fM", num / 1e6):gsub("%.0M","M")
    elseif num >= 1e3 then
        return string.format("%.1fK", num / 1e3):gsub("%.0K","K")
    else
        return tostring(num)
    end
end

if isCommand("stock") then
    local totalCash = 0

    for _, userId in pairs(getgenv().Config.Alts or {}) do
        local alt = Players:GetPlayerByUserId(userId)
        if alt and alt:FindFirstChild("DataFolder") and alt.DataFolder:FindFirstChild("Currency") then
            totalCash += tonumber(alt.DataFolder.Currency.Value) or 0
        end
    end

    if TextChatService.TextChannels:FindFirstChild("RBXGeneral") then
        TextChatService.TextChannels.RBXGeneral:SendAsync("Stock: $" .. formatAbbrev(totalCash))
    end
end

if isCommand("airlock") then
    game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("Airlocked")
  local IgnoredVelocity = game.Players.LocalPlayer.Character.HumanoidRootPart:FindFirstChild("IgnoredVelocity")
  if IgnoredVelocity then
    IgnoredVelocity:Destroy()
  end
  local BP = Instance.new("BodyPosition")
  BP.Name = "IgnoredVelocity"
  BP:SetAttribute("AllowedBM", true)
  BP.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
  BP.D = 1000
  BP.P = 30000
  BP.Position = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0,tonumber(SplitCMD[2]) or 5,0).Position
  BP.Parent = game.Players.LocalPlayer.Character.HumanoidRootPart
end

if isCommand("unairlock") then
    game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("Unairlocked")
    game.Players.LocalPlayer.Character.HumanoidRootPart:FindFirstChild("IgnoredVelocity"):Destroy()
    game.Players.LocalPlayer.Character.Animate.Disabled = false
    for _, track in pairs(game.Players.LocalPlayer.Character.Humanoid:GetPlayingAnimationTracks()) do
        track:Stop()
    end
    while game.Players.LocalPlayer.Character.Humanoid.FloorMaterial == Enum.Material.Air do
        task.wait()
    end
end

     if isCommand("render") then
 game:GetService("RunService"):Set3dRenderingEnabled(true)
     end

     if isCommand("unrender") then
 game:GetService("RunService"):Set3dRenderingEnabled(false)
    end

    if isCommand("cashaura") then
        game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("Cashaura enabled")
        if not CashAuraEnabled then
            CashAuraEnabled = true
            getgenv().CashAura = game:GetService("RunService").Heartbeat:Connect(function()
                for _, v in pairs(game:GetService('Workspace')['Ignored']['Drop']:GetChildren()) do
                    if v:IsA('Part') then
                        if (v.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 25 then
                            fireclickdetector(v:FindFirstChild('ClickDetector'))
                        end
                    end
                end
            end)
        end
    end
    
    if isCommand("uncashaura") then
        game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("Cashaura disabled")
        if CashAuraEnabled then
            CashAuraEnabled = false
            if getgenv().CashAura then
                getgenv().CashAura:Disconnect()
                getgenv().CashAura = nil
            end
        end
    end
end

game:GetService("TextChatService").MessageReceived:Connect(function(m)
        if m.TextSource and game.Players:GetPlayerByUserId(m.TextSource.UserId).Name == getgenv().Config.Host then
            commands(m.Text, m.TextSource.UserId)
        end
    end)

    game.Players.PlayerAdded:Connect(function(p)
        if p.Name == getgenv().Config.Host then
            Notify({
                Description = "The host (" .. p.Name .. ") has Rejoined.",
                Title = "Host Rejoined",
                Duration = 3
            })
            game:GetService("TextChatService").MessageReceived:Connect(function(m)
                if m.TextSource and game.Players:GetPlayerByUserId(m.TextSource.UserId).Name == getgenv().Config.Host then
                    commands(m.Text, m.TextSource.UserId)
                end
            end)
        end
    end)

