if Host then
   Notify({
   Description = "The host (" .. getgenv().Config.Host .. ") was found.",
   Title = "Host Found!",
   Duration = 5
 })
game.TextChatService.ChatWindowConfiguration.Enabled = true

game:GetService("RunService"):Set3dRenderingEnabled(false)

if not setfpscap then  
    if tonumber(getgenv().Config.Fps) then
        _G.FPS = getgenv().Config.Fps
        local clock = tick()

        _G.fps = true
        game:GetService("RunService").Heartbeat:Connect(function()
            if _G.fps then
                while clock + 1 / _G.FPS > tick() do end
                task.wait()
                clock = tick()
            end
        end)
    end
else
    setfpscap(getgenv().Config.Fps)
end  
else
    Notify({
    Description = "The host (" .. getgenv().Config.Host .. ") was not found.",
    Title = "Host Not Found",
    Duration = 3
 })
end

function commands(msg, plr)
    local Msg = string.lower(msg)
    local SplitCMD = string.split(Msg, " ")
    local Prefix = getgenv().Config.Prefix
    
    local function isCommand(cmd)
        return SplitCMD[1] == Prefix .. cmd or string.find(Msg, "/e " .. cmd)
    end

    if isCommand("bring") then
        game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("Brought alts")
      game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Host.Character.HumanoidRootPart.CFrame
    end
      
    if isCommand("say") then
      game.TextChatService.TextChannels.RBXGeneral:SendAsync(msg:sub(#SplitCMD[1] + 2))
    end

local ts = game:GetService("TweenService")
local root = game.Players.LocalPlayer.Character.HumanoidRootPart
local info = TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)

if isCommand("circle") then
    game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("Circled")
    local alts = getgenv().Config.Alts
    local radius = 10
    local count = 0

    for _ in pairs(alts) do count += 1 end
    local angleStep = (2 * math.pi) / count
    local i = 0

    for _, v in pairs(alts) do
        if v == game.Players.LocalPlayer.UserId then
            local angle = i * angleStep
            local position = Host.Character.HumanoidRootPart.Position + Vector3.new(math.cos(angle) * 20, 0, math.sin(angle) * radius)
            ts:Create(root, info, {CFrame = CFrame.new(position, Host.Character.HumanoidRootPart.Position)}):Play()
        end
        i += 1
    end
end

