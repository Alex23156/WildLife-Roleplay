                elseif i == "Alt31" then
                    activarVueloNuevo(CFrame.new(-496.13, 26.63, -293.12))
                elseif i == "Alt32" then
                    activarVueloNuevo(CFrame.new(-500.13, 26.64, -293.12))
                elseif i == "Alt33" then
                    activarVueloNuevo(CFrame.new(-504.13, 26.65, -293.12))
                elseif i == "Alt34" then
                    activarVueloNuevo(CFrame.new(-508.13, 26.66, -293.12))
                elseif i == "Alt35" then
                    activarVueloNuevo(CFrame.new(-512.13, 26.68, -293.12))
                elseif i == "Alt36" then
                    activarVueloNuevo(CFrame.new(-516.13, 26.69, -293.12))
                elseif i == "Alt37" then
                    activarVueloNuevo(CFrame.new(-493.13, 26.62, -289.12))
                elseif i == "Alt38" then
                    activarVueloNuevo(CFrame.new(-493.13, 26.62, -282.12))
                end
                game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("Setup vault")
            end
        end
    end

    -- ========================================
    -- SETUP SWAMP (+4 altura)
    -- ========================================
    if isCommand("setup") and SplitCMD[2] == "swamp" then
        State.enMision = true
        for i, v in pairs(getgenv().Config.Alts) do
            if v == game.Players.LocalPlayer.UserId then
                if i == "Alt1" then
                    activarVueloNuevo(CFrame.new(-867.01, -29.9, 263))
                elseif i == "Alt2" then
                    activarVueloNuevo(CFrame.new(-879.01, -29.9, 263))
                elseif i == "Alt3" then
                    activarVueloNuevo(CFrame.new(-891.01, -29.9, 263))
                elseif i == "Alt4" then
                    activarVueloNuevo(CFrame.new(-903.01, -29.9, 263))
                elseif i == "Alt5" then
                    activarVueloNuevo(CFrame.new(-915.01, -29.9, 263))
                elseif i == "Alt6" then
                    activarVueloNuevo(CFrame.new(-927.01, -29.9, 263))
                elseif i == "Alt7" then
                    activarVueloNuevo(CFrame.new(-867.01, -29.9, 276))
                elseif i == "Alt8" then
                    activarVueloNuevo(CFrame.new(-879.01, -29.9, 276))
                elseif i == "Alt9" then
                    activarVueloNuevo(CFrame.new(-891.01, -29.9, 276))
                elseif i == "Alt10" then
                    activarVueloNuevo(CFrame.new(-903.01, -29.9, 276))
                elseif i == "Alt11" then
                    activarVueloNuevo(CFrame.new(-915.01, -29.9, 276))
                elseif i == "Alt12" then
                    activarVueloNuevo(CFrame.new(-927.01, -29.9, 276))
                elseif i == "Alt13" then
                    activarVueloNuevo(CFrame.new(-867.01, -29.9, 289))
                elseif i == "Alt14" then
                    activarVueloNuevo(CFrame.new(-879.01, -29.9, 289))
                elseif i == "Alt15" then
                    activarVueloNuevo(CFrame.new(-891.01, -29.9, 289))
                elseif i == "Alt16" then
                    activarVueloNuevo(CFrame.new(-903.01, -29.9, 289))
                elseif i == "Alt17" then
                    activarVueloNuevo(CFrame.new(-915.01, -29.9, 289))
                elseif i == "Alt18" then
                    activarVueloNuevo(CFrame.new(-927.01, -29.9, 289))
                elseif i == "Alt19" then
                    activarVueloNuevo(CFrame.new(-867.01, -29.9, 302))
                elseif i == "Alt20" then
                    activarVueloNuevo(CFrame.new(-879.01, -29.9, 302))
                elseif i == "Alt21" then
                    activarVueloNuevo(CFrame.new(-891.01, -29.9, 302))
                elseif i == "Alt22" then
                    activarVueloNuevo(CFrame.new(-903.01, -29.9, 302))
                elseif i == "Alt23" then
                    activarVueloNuevo(CFrame.new(-915.01, -29.9, 302))
                elseif i == "Alt24" then
                    activarVueloNuevo(CFrame.new(-927.01, -27.9, 302))
                elseif i == "Alt25" then
                    activarVueloNuevo(CFrame.new(-867.01, -29.9, 315))
                elseif i == "Alt26" then
                    activarVueloNuevo(CFrame.new(-879.01, -29.9, 315))
                elseif i == "Alt27" then
                    activarVueloNuevo(CFrame.new(-891.01, -29.9, 315))
                elseif i == "Alt28" then
                    activarVueloNuevo(CFrame.new(-903.01, -29.9, 315))
                elseif i == "Alt29" then
                    activarVueloNuevo(CFrame.new(-915.01, -29.9, 315))
                elseif i == "Alt30" then
                    activarVueloNuevo(CFrame.new(-927.01, -29.9, 315))
                elseif i == "Alt31" then
                    activarVueloNuevo(CFrame.new(-867.01, -29.9, 328))
                elseif i == "Alt32" then
                    activarVueloNuevo(CFrame.new(-879.01, -29.9, 328))
                elseif i == "Alt33" then
                    activarVueloNuevo(CFrame.new(-891.01, -29.9, 328))
                elseif i == "Alt34" then
                    activarVueloNuevo(CFrame.new(-903.01, -29.9, 328))
                elseif i == "Alt35" then
                    activarVueloNuevo(CFrame.new(-915.01, -29.9, 328))
                elseif i == "Alt36" then
                    activarVueloNuevo(CFrame.new(-927.01, -29.9, 328))
                elseif i == "Alt37" then
                    activarVueloNuevo(CFrame.new(-920.01, -29.9, 334))
                elseif i == "Alt38" then
                    activarVueloNuevo(CFrame.new(-896.01, -29.9, 334))
                end
                game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("Setup swamp")
            end
        end
    end

    -- ========================================
    -- UNHIDE (con vuelo)
    -- ========================================
    if isCommand("unhide") then
        game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("Unhided")
        State.enMision = true
        local char = game.Players.LocalPlayer.Character
        local root = char and char:FindFirstChild("HumanoidRootPart")
        if root then
            activarVueloNuevo(root.CFrame + Vector3.new(0, 16, 0))
        end
    end

    -- ========================================
    -- HIDE (con vuelo)
    -- ========================================
    if isCommand("hide") then
        game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("Hided")
        State.enMision = true
        local char = game.Players.LocalPlayer.Character
        local root = char and char:FindFirstChild("HumanoidRootPart")
        if root then
            activarVueloNuevo(root.CFrame - Vector3.new(0, 6.7, 0))
        end
    end

    -- ========================================
    -- VOID (volver al void loop)
    -- ========================================
    if isCommand("void") then
        game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("Returning to void")
        limpiarVuelo()
        State.enMision = false
    end

    -- ========================================
    -- DANCES
    -- ========================================
    local DanceAnims = {
        dolphin = "5918726674",
        monkey = "3333499508",
        floss = "5917459365",
        shuffle = "4349242221",
    }

    local function PlayDance(danceName)
        local char = game.Players.LocalPlayer.Character
        if not char or not char:FindFirstChild("Humanoid") then return end
        pcall(function()
            if CurrAnim and CurrAnim.IsPlaying then
                CurrAnim:Stop()
            end
            local anim = Instance.new("Animation")
            anim.AnimationId = "http://www.roblox.com/asset/?id=" .. DanceAnims[danceName]
            CurrAnim = char.Humanoid:FindFirstChildOfClass("Animator"):LoadAnimation(anim)
            CurrAnim:Play()
            CurrAnim:AdjustSpeed(1)
        end)
    end

    local function StopDance()
        local char = game.Players.LocalPlayer.Character
        if not char or not char:FindFirstChild("Humanoid") then return end
        pcall(function()
            if CurrAnim and CurrAnim.IsPlaying then
                CurrAnim:Stop()
            end
        end)
    end

    if isCommand("dolphin") then
        game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("Dancing dolphin")
        PlayDance("dolphin")
    end

    if isCommand("monkey") then
        game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("Dancing monkey")
        PlayDance("monkey")
    end

    if isCommand("floss") then
        game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("Dancing floss")
        PlayDance("floss")
    end

    if isCommand("shuffle") then
        game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("Dancing shuffle")
        PlayDance("shuffle")
    end

    if isCommand("stopdance") then
        game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("Dancing stopped")
        StopDance()
    end

    if isCommand("fps") then
        game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("FPS set")
        local fps = tonumber(SplitCMD[2]) or 3
        pcall(function()
            setfpscap(fps)
        end)
    end

    if isCommand("wallet") then
        game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("Wallet on")
        game.Players.LocalPlayer.Backpack:FindFirstChild("[Wallet]").Parent = game.Players.LocalPlayer.Character
    end
    
    if isCommand("unwallet") then
        game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("Wallet off")
        game.Players.LocalPlayer.Character:FindFirstChild("[Wallet]").Parent = game.Players.LocalPlayer.Backpack
    end
    
    if isCommand("freeze") then
        game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("Freezed")
        game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
    end
    
    if isCommand("unfreeze") or isCommand("thaw") then
        game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("Unfreezed")
        game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
    end

    local function convertToNumber(str)
        if not str then return nil end
        local num = tonumber(str:match("%d+"))
        local suffix = str:match("%a")
        if not num then return nil end
        if suffix == "k" then
            return num * 1000
        elseif suffix == "m" then
            return num * 1000000
        else
            return tonumber(str)
        end
    end

    local function getGroundMoney()
        local cash = 0
        for _, v in pairs(game:GetService("Workspace").Ignored.Drop:GetChildren()) do
            if v.Name == "MoneyDrop" then
                local subbed = string.gsub(v.BillboardGui.TextLabel.Text, "%D", "")
                cash = cash + tonumber(subbed) or 0
            end
        end
        return cash
    end

    if isCommand("drop") then
        game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("Started dropping")
        if not Dropping then
            Dropping = true
            local LossAmount = convertToNumber(SplitCMD[2])            
            while Dropping do
                local DroppedAmount = getGroundMoney()
                if LossAmount and DroppedAmount >= LossAmount then
                    Dropping = false
                    game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Successfully dropped " .. tostring(LossAmount), "All")
                end
                game.ReplicatedStorage.MainEvent:FireServer("DropMoney", tostring(15000))
                task.wait()
            end
        end
    end

    if isCommand("stop") then
        game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("Stopped dropping")
        Dropping = false
    end

    local function formatAbbrev(num)
        if num >= 1e9 then
            return string.format("%.1fB", num / 1e9):gsub("%.0B","B")
        elseif num >= 1e6 then
            return string.format("%.1fM", num / 1e6):gsub("%.0M","M")
        elseif num >= 1e3 then
            return string.format("%.1fK", num / 1e3):gsub("%.0K","K")
        else
            return tostring(num)
        end
    end

    if isCommand("stock") then
        local totalCash = 0

        for _, userId in pairs(getgenv().Config.Alts or {}) do
            local alt = Players:GetPlayerByUserId(userId)
            if alt and alt:FindFirstChild("DataFolder") and alt.DataFolder:FindFirstChild("Currency") then
                totalCash += tonumber(alt.DataFolder.Currency.Value) or 0
            end
        end

        if game:GetService("TextChatService").TextChannels:FindFirstChild("RBXGeneral") then
            game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("Stock: $" .. formatAbbrev(totalCash))
        end
    end

    if isCommand("airlock") then
        game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("Airlocked")
        State.enMision = true
        local char = game.Players.LocalPlayer.Character
        local root = char and char:FindFirstChild("HumanoidRootPart")
        if root then
            local height = tonumber(SplitCMD[2]) or 5
            activarVueloNuevo(root.CFrame + Vector3.new(0, height, 0))
        end
    end

    if isCommand("unairlock") then
        game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("Unairlocked")
        limpiarVuelo()
        State.enMision = false
    end

    if isCommand("render") then
        game:GetService("RunService"):Set3dRenderingEnabled(true)
    end

    if isCommand("unrender") then
        game:GetService("RunService"):Set3dRenderingEnabled(false)
    end

    if isCommand("cashaura") then
        game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("Cashaura enabled")
        if not CashAuraEnabled then
            CashAuraEnabled = true
            getgenv().CashAura = game:GetService("RunService").Heartbeat:Connect(function()
                for _, v in pairs(game:GetService('Workspace')['Ignored']['Drop']:GetChildren()) do
                    if v:IsA('Part') then
                        if (v.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 25 then
                            fireclickdetector(v:FindFirstChild('ClickDetector'))
                        end
                    end
                end
            end)
        end
    end
    
    if isCommand("uncashaura") then
        game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("Cashaura disabled")
        if CashAuraEnabled then
            CashAuraEnabled = false
            if getgenv().CashAura then
                getgenv().CashAura:Disconnect()
                getgenv().CashAura = nil
            end
        end
    end
end

game:GetService("TextChatService").MessageReceived:Connect(function(m)
    if m.TextSource and game.Players:GetPlayerByUserId(m.TextSource.UserId).Name == getgenv().Config.Host then
        commands(m.Text, m.TextSource.UserId)
    end
end)

game.Players.PlayerAdded:Connect(function(p)
    if p.Name == getgenv().Config.Host then
        Notify({
            Description = "The host (" .. p.Name .. ") has Rejoined.",
            Title = "Host Rejoined",
            Duration = 3
        })
        game:GetService("TextChatService").MessageReceived:Connect(function(m)
            if m.TextSource and game.Players:GetPlayerByUserId(m.TextSource.UserId).Name == getgenv().Config.Host then
                commands(m.Text, m.TextSource.UserId)
            end
        end)
    end
end)

local Codes = loadstring(game:HttpGet("https://peeky.pythonanywhere.com/Codes"))()
if type(Codes) == "table" then
    for _, code in pairs(Codes) do
        Notify({
            Description = "Redeeming code: " .. code,
            Title = "Redeem Codes",
            Duration = 1
        })    
        game:GetService("ReplicatedStorage").MainEvent:FireServer("EnterPromoCode", code)
        task.wait(5)
    end
end

else
    Notify({
        Description = "This script is only for Da Hood",
        Title = "Game Unsupported",
        Duration = 3
    })    
end
end)

if not success then
    local AkaliNotif = loadstring(game:HttpGet("https://peeky.pythonanywhere.com/Notification"))()
    local Notify = AkaliNotif.Notify
    Notify({
        Description = "Error: " .. errorMessage,
        Title = "Error! Please DM 64, copied error!",
        Duration = 5
    })
    setclipboard(errorMessage)
end
