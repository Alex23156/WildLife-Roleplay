local success, errorMessage = pcall(function()
local AkaliNotif = loadstring(game:HttpGet("https://peeky.pythonanywhere.com/Notification"))()
local Notify = AkaliNotif.Notify

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local lp = Players.LocalPlayer
local host = (getgenv().Config and getgenv().Config.Host) or nil

if host and lp and lp.Name == host then
    return
end

-- ========================================
-- FLIGHT SYSTEM 
-- ========================================
local State = {
    Vuelo = {
        vueloThread = nil,
        vueloActivo = false,
        posVuelo = false,
        Core = nil,
    },
    enMision = false -- Para controlar el void loop
}

local function limpiarVuelo()
    State.Vuelo.vueloActivo = false
    State.Vuelo.posVuelo = nil
    State.Vuelo.vueloThread = false

    if workspace:FindFirstChild("Core") then
        workspace.Core:Destroy()
    end

    local char = lp.Character
    if char then
        local root = char:FindFirstChild("HumanoidRootPart")
        if root then root.CanCollide = true end
        local humanoid = char:FindFirstChild("Humanoid")
        if humanoid then humanoid.PlatformStand = false end
    end
end

local function activarVueloNuevo(targetCFrame)
    local character = lp.Character or lp.CharacterAdded:Wait()
    local root = character:WaitForChild("HumanoidRootPart")

    limpiarVuelo()
    task.wait()

    if workspace:FindFirstChild("Core") then
        workspace.Core:Destroy()
    end

    local Core = Instance.new("Part")
    Core.Name = "Core"
    Core.Size = Vector3.new(0.05, 0.05, 0.05)
    Core.Anchored = false
    Core.CanCollide = false
    Core.Transparency = 1
    Core.Parent = workspace

    local weld = Instance.new("Weld", Core)
    weld.Part0 = Core
    weld.Part1 = character:WaitForChild("LowerTorso")
    weld.C0 = CFrame.new(0, 0, 0)

    local torso = Core
    State.Vuelo.Core = Core
    State.Vuelo.vueloActivo = true
    State.Vuelo.vueloThread = true

    root.CanCollide = false
    local humanoid = character:WaitForChild("Humanoid")
    humanoid.PlatformStand = true

    task.spawn(function()
        local pos = Instance.new("BodyPosition", torso)
        local gyro = Instance.new("BodyGyro", torso)
        pos.Name = "EPIXPOS"
        pos.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
        pos.Position = targetCFrame.Position
        gyro.MaxTorque = Vector3.new(9e9, 9e9, 9e9)
        gyro.CFrame = targetCFrame

        while State.Vuelo.vueloActivo do
            if State.Vuelo.posVuelo then
                pos.Position = State.Vuelo.posVuelo.Position
                gyro.CFrame = State.Vuelo.posVuelo
            else
                pos.Position = targetCFrame.Position
                gyro.CFrame = targetCFrame
            end
            task.wait()
        end
    end)
    task.wait(1)
end

local function flyToRandomPosition()
    local char = lp.Character or lp.CharacterAdded:Wait()
    if not char then return end
    
    local root = char:FindFirstChild("HumanoidRootPart")
    if not root then return end
    
    local randomPosition = Vector3.new(
        math.random(-38000000, 38000000), 
        math.random(0, 20000000), 
        math.random(-38000000, 38000000)
    )
    
    root.CFrame = CFrame.new(randomPosition)
    
    local humanoid = char:FindFirstChildOfClass("Humanoid")
    if humanoid then 
        humanoid:ChangeState(Enum.HumanoidStateType.Freefall) 
    end
end

-- ========================================
-- VOID PROTECTION LOOP
-- ========================================
task.spawn(function()
    while true do
        if not State.enMision then
            flyToRandomPosition()
        end
        task.wait(5)
    end
end)

-- ========================================
-- ORIGINAL SCRIPT CONTINUES
-- ========================================

local fired = false
game:GetService("Players").LocalPlayer.CharacterAdded:Connect(function()
    fired = false
end)

getgenv().fireclickdetector = function(object, distance, event)
    if not fired then
        fired = true
        game:GetService("VirtualInputManager"):SendKeyEvent(true, 101, false, peeky)
    end

    if game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Tool") then
        game:GetService("Players").LocalPlayer.Character.Humanoid:UnequipTools()
    end

    local click_detector = object:FindFirstChild("ClickDetector") or object
    local old_cd_parent = click_detector.Parent

    local stub_part = Instance.new("Part")
    stub_part.Transparency = 1
    stub_part.Size = Vector3.new(30, 30, 30)
    stub_part.Anchored = true
    stub_part.CanCollide = false
    stub_part.Parent = workspace

    click_detector.Parent = stub_part
    click_detector.MaxActivationDistance = math.huge

    local connection = game:GetService("RunService").Heartbeat:Connect(function()
        stub_part.CFrame = workspace.Camera.CFrame * CFrame.new(0, 0, -20) * CFrame.new(workspace.Camera.CFrame.LookVector)
        game:GetService("VirtualUser"):ClickButton1(Vector2.new(20, 20), workspace:FindFirstChildOfClass("Camera").CFrame)
    end)
    click_detector.MouseClick:Once(function()
        connection:Disconnect()
        click_detector.Parent = old_cd_parent
        stub_part:Destroy()
    end)

    task.delay(3, function()
        connection:Disconnect()
        click_detector.Parent = old_cd_parent
        stub_part:Destroy()
    end)
end

