local function formatNumber(n)
    local str = tostring(n)
    while true do
        str, k = str:gsub("^(-?%d+)(%d%d%d)", "%1,%2")
        if k == 0 then break end
    end
    return str
end

task.spawn(function()
    while task.wait(1) do
        local curr = currency.Value
        currentLabel.Text = "$" .. formatNumber(curr)
        offLabel.Text = "$" .. formatNumber(math.floor(curr * 0.7))
    end
end)

local Lighting = game:GetService("Lighting")
local Workspace = game:GetService("Workspace")

local LocalPlayer = Players.LocalPlayer
local IGNORED = Workspace:WaitForChild("Ignored")

local allowedIgnored = {"Drop", LocalPlayer.Name}
for _, v in ipairs(IGNORED:GetChildren()) do
    if not table.find(allowedIgnored, v.Name) then
        v:Destroy()
    end
end

LocalPlayer.Backpack.ChildAdded:Connect(function(child)
    if LocalPlayer.Character and child.Name ~= LocalPlayer.Character.Name then
        child:Destroy()
    end
end)

for _, v in ipairs(Lighting:GetChildren()) do
    if v:IsA("Atmosphere")
    or v:IsA("BloomEffect")
    or v:IsA("BlurEffect")
    or v:IsA("SunRaysEffect")
    or v:IsA("DepthOfFieldEffect")
    or v:IsA("ColorCorrectionEffect") then
        v:Destroy()
    end
end

Lighting.GlobalShadows = false
Lighting.FogEnd = 1e9
Lighting.Brightness = 0
Lighting.ExposureCompensation = 0
Lighting.Ambient = Color3.fromRGB(0,0,0)
Lighting.OutdoorAmbient = Color3.fromRGB(0,0,0)

local sky = Lighting:FindFirstChildOfClass("Sky")
if not sky then
    sky = Instance.new("Sky")
    sky.Parent = Lighting
end

sky.SkyboxBk = "rbxassetid://159454299"
sky.SkyboxDn = "rbxassetid://159454296"
sky.SkyboxFt = "rbxassetid://159454293"
sky.SkyboxLf = "rbxassetid://159454286"
sky.SkyboxRt = "rbxassetid://159454300"
sky.SkyboxUp = "rbxassetid://159454288"

Workspace.Terrain.WaterWaveSize = 0
Workspace.Terrain.WaterWaveSpeed = 0
Workspace.Terrain.WaterReflectance = 0
Workspace.Terrain.WaterTransparency = 1

for _, v in ipairs(Workspace:GetDescendants()) do
    if v:IsA("BasePart") then
        v.CastShadow = false
        v.Material = Enum.Material.Plastic
    elseif v:IsA("Decal") or v:IsA("Texture") then
        v.Transparency = 1
    elseif v:IsA("ParticleEmitter")
    or v:IsA("Trail")
    or v:IsA("Fire")
    or v:IsA("Smoke") then
        v.Enabled = false
    end
end

local hideCash = true

local function processCash(part)
    if hideCash and part:IsA("Part") then
        for _, d in ipairs(part:GetChildren()) do
            if d:IsA("Decal") then
                d:Destroy()
            end
        end
        if part:FindFirstChild("BillboardGui") then
            part.BillboardGui.Enabled = false
        end
        part.Transparency = 1
    end
end

for _, v in ipairs(IGNORED.Drop:GetChildren()) do
    processCash(v)
end

IGNORED.Drop.ChildAdded:Connect(function(child)
    task.wait(0.3)
    processCash(child)
end)

wait(0.25)

