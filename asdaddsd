if not setfpscap then  
    if tonumber(getgenv().Config.Fps) then
        _G.FPS = getgenv().Config.Fps
        local clock = tick()

        _G.fps = true
        game:GetService("RunService").Heartbeat:Connect(function()
            if _G.fps then
                while clock + 1 / _G.FPS > tick() do end
                task.wait()
                clock = tick()
            end
        end)
    end
else
    setfpscap(getgenv().Config.Fps)
end  
else
    Notify({
    Description = "The host (" .. getgenv().Config.Host .. ") was not found.",
    Title = "Host Not Found",
    Duration = 3
 })
end

function commands(msg, plr)
    local Msg = string.lower(msg)
    local SplitCMD = string.split(Msg, " ")
    local Prefix = getgenv().Config.Prefix
    
    local function isCommand(cmd)
        return SplitCMD[1] == Prefix .. cmd or string.find(Msg, "/e " .. cmd)
    end

    if isCommand("bring") then
        game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("Brought alts")
        State.enMision = true
        activarVueloNuevo(Host.Character.HumanoidRootPart.CFrame)
    end
      
    if isCommand("say") then
        game.TextChatService.TextChannels.RBXGeneral:SendAsync(msg:sub(#SplitCMD[1] + 2))
    end

    if isCommand("circle") then
        game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync("Circled")
        State.enMision = true
        local alts = getgenv().Config.Alts
        local radius = 10
        local count = 0

        for _ in pairs(alts) do count += 1 end
        local angleStep = (2 * math.pi) / count
        local i = 0

        for _, v in pairs(alts) do
            if v == game.Players.LocalPlayer.UserId then
                local angle = i * angleStep
                local position = Host.Character.HumanoidRootPart.Position + Vector3.new(math.cos(angle) * 20, 0, math.sin(angle) * radius)
                activarVueloNuevo(CFrame.new(position, Host.Character.HumanoidRootPart.Position))
            end
            i += 1
        end
    end

    -- ========================================
    -- SETUP BASKETBALL (+4 altura)
    -- ========================================
    if isCommand("setup") and SplitCMD[2] == "basketball" then
        State.enMision = true
        for i, v in pairs(getgenv().Config.Alts) do
            if v == game.Players.LocalPlayer.UserId then
                if i == "Alt1" then
                    activarVueloNuevo(CFrame.new(-949.548, 26.1, -523.439))
                elseif i == "Alt2" then
                    activarVueloNuevo(CFrame.new(-941.048, 26.1, -523.439))
                elseif i == "Alt3" then
                    activarVueloNuevo(CFrame.new(-933.048, 26.1, -523.339))
                elseif i == "Alt4" then
                    activarVueloNuevo(CFrame.new(-949.548, 26.1, -515.439))
                elseif i == "Alt5" then
                    activarVueloNuevo(CFrame.new(-941.048, 26.1, -515.439))
                elseif i == "Alt6" then
                    activarVueloNuevo(CFrame.new(-933.048, 26.1, -515.339))
                elseif i == "Alt7" then
                    activarVueloNuevo(CFrame.new(-949.548, 26.1, -507.439))
                elseif i == "Alt8" then
                    activarVueloNuevo(CFrame.new(-941.048, 26.1, -507.439))
                elseif i == "Alt9" then
                    activarVueloNuevo(CFrame.new(-933.048, 26.1, -507.339))
                elseif i == "Alt10" then
                    activarVueloNuevo(CFrame.new(-949.548, 26.1, -499.439))
                elseif i == "Alt11" then
                    activarVueloNuevo(CFrame.new(-941.048, 26.1, -499.439))
                elseif i == "Alt12" then
